<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="common" targetRuntime="MyBatis3">
		<property name="javaFileEncoding" value="UTF-8" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:./build/mbgenerator" userId="sa" />
		<javaModelGenerator targetPackage="cherry.sqlapp.db.gen.dto"
			targetProject="src/generated/java" />
		<sqlMapGenerator targetPackage="cherry.sqlapp.db.gen.mapper"
			targetProject="src/generated/java" />
		<javaClientGenerator targetPackage="cherry.sqlapp.db.gen.mapper"
			type="ANNOTATEDMAPPER" targetProject="src/generated/java" />
		<table tableName="sqltool_load">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
		<table tableName="sqltool_statement">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
		<table tableName="sqltool_clause">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
		<table tableName="sqltool_metadata">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="sql_type"
				javaType="cherry.sqlapp.code.SqlTypeCode" />
			<columnOverride column="published_flg"
				javaType="cherry.spring.common.custom.FlagCode" />
			<columnOverride column="registered_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
		<table tableName="async_proc">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="registered_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="invoked_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="started_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="finished_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
		<table tableName="bizdatetime_master">
			<generatedKey column="id" sqlStatement="JDBC" />
			<columnOverride column="bizdate" javaType="org.joda.time.LocalDate" />
			<columnOverride column="updated_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="created_at" javaType="org.joda.time.LocalDateTime" />
			<columnOverride column="deleted_flg"
				javaType="cherry.spring.common.custom.DeletedFlag" />
		</table>
	</context>
</generatorConfiguration>
