<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="common" targetRuntime="MyBatis3Simple">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:./build/mbgenerator" userId="sa" />
		<javaModelGenerator targetPackage="cherry.sqlapp.db.gen.dto"
			targetProject="common/src/main/java">
			<property name="rootClass" value="cherry.sqlapp.db.BaseDto" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="cherry.sqlapp.db.gen.mapper"
			targetProject="src/main/java" />
		<javaClientGenerator targetPackage="cherry.sqlapp.db.gen.mapper"
			type="XMLMAPPER" targetProject="src/main/java" />
		<table tableName="sql_metadata" />
		<table tableName="sql_select" />
	</context>
</generatorConfiguration>
